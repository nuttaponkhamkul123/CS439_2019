   "source": [
    "def decorator(func):\n",
    "    def wrapper():\n",
    "        func()\n",
    "        print \"Hello world\"\n",
    "    return wrapper\n",
    "\n",
    "@decorator\n",
    "def phello():\n",
    "    print(\"this is p function\")\n",
    "\n",
    "val = decorator(phello)"
   ]
